
@model List<RightOneCRM.Models.CompanyOwners>
<style>
    .btnCD {
        margin: 5px;
        padding: 5px;
    }
</style>
<section class="py-5">
    <div class="row">
        <div class="col-sm-12">
            <table class="table table-bordered table-hover table-striped">
                <thead>
                    <tr>
                        <th>Owner Name</th>
                        <th>Company Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var owner in Model)
                    {
                        <tr>
                            <td>@owner.OwnerName</td>
                            <td>@owner.CompanyName</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</section>

